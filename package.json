{
  "name": "rss-feeder",
  "version": "0.0.0",
  "description": "A handsome RSS feed for your home page",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --watch --mode development",
    "server": "webpack-dev-server && start http://localhost:8080/",
    "deploy": "webpack --mode production",
    "deploy-ext": "web-ext build -i ./src ./webpack.config.js ./gitignore ./package-lock.json ./babel.config.json ./index-dev.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalNaut/rss-feeder.git"
  },
  "keywords": [
    "rss",
    "feed"
  ],
  "author": "DigitalNaut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DigitalNaut/rss-feeder/issues"
  },
  "homepage": "https://github.com/DigitalNaut/rss-feeder#readme",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-react": "^7.10.4",
    "css-loader": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
